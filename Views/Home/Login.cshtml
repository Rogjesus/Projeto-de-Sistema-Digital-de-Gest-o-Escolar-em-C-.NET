@model DigitalSchoolMannagementSYstem.Models.TblUser
@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}


<center>
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    @using (Html.BeginForm("Login", "Home"))
    {

        <table borger="1" backColor="Yellow">
            <tr>
                <td align="center" colspan="2">LogIn</td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.userName)</td>
                <td>@Html.TextBoxFor(m => m.userName)</td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.password)</td>
                <td>@Html.TextBoxFor(m => m.password)</td>
            </tr>
            <tr>
                <td colspan="2" align="center">
                    <input type="Submit" value="Login" /><spla>&nbsp;</spla>
                    <input type="Submit" value="SignUp" id="btnSignUp" />

                </td>
            </tr>
        </table>

    }
